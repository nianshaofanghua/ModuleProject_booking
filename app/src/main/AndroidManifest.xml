<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="may.com.module">
    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:name=".app.MyApplication"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true"
        android:theme="@style/SampleTheme.Light"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <meta-data
            android:name="DATABASE"
            android:value="user.db" />
        <meta-data
            android:name="VERSION"
            android:value="1" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="may.com.module" />

        <activity android:name=".app.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.RegisterActivity" />
        <activity android:name=".app.MainActivity" />
        <activity android:name=".app.SaveMoneyActivity" />
        <activity android:name=".app.AddMoneyAimActivity" />
        <activity android:name=".app.MoneyListActivity" />
        <activity android:name=".app.AddMoneyActivity" />
        <activity android:name=".app.StatisticsMoneyActivity"></activity>
    </application>

</manifest>